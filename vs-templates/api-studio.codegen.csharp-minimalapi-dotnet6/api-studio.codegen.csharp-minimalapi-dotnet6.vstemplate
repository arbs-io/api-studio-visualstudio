<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="ProjectGroup">
  <TemplateData>
    <Name>ASP.NET Core Web API (Api Studio)</Name>
    <Description>A project template for creating an ASP.NET Core application. This template uses Minimal Api to help accelerate the creation of .NET API projects</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>Api Studio</ProjectSubType>
    <SortOrder>5000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>WebApplication</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>api-studio.codegen.csharp-minimalapi-dotnet6.ico</Icon>
    <PreviewImage>api-studio.codegen.csharp-minimalapi-dotnet6.ico</PreviewImage>
    <LanguageTag>C#</LanguageTag>
    <ProjectTypeTag>Api Studio</ProjectTypeTag>
    <ProjectTypeTag>Linux</ProjectTypeTag>
    <ProjectTypeTag>macOS</ProjectTypeTag>
    <ProjectTypeTag>Windows</ProjectTypeTag>
    <ProjectTypeTag>Cloud</ProjectTypeTag>
    <ProjectTypeTag>Service</ProjectTypeTag>
    <ProjectTypeTag>Web</ProjectTypeTag>
    <ProjectTypeTag>WebAPI</ProjectTypeTag>
  </TemplateData>
  <TemplateContent>
    <ProjectCollection>
      <ProjectTemplateLink ProjectName="$safeprojectname$" CopyParameters="true">
        WebApplication\WebApplication.vstemplate
      </ProjectTemplateLink>
    </ProjectCollection>
  </TemplateContent>
</VSTemplate>